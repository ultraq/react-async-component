{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"ultraq"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"rules": {
		"no-unused-vars": ["error", {
			"ignoreRestSiblings": true
		}]
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
